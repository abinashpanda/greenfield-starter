{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@apollo/client": "^3.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "antd": "^4.5.4",
    "apollo": "^2.30.2",
    "axios": "^0.19.2",
    "clsx": "^1.1.1",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "graphql:generate-types": "apollo codegen:generate src/types --target=typescript  --outputFlat --watch"
  },
  "eslintConfig": {
    "extends": "react-app",
    "settings": {
      "import/resolver": {
        "node": {
          "path": [
            "src"
          ]
        }
      }
    },
    "rules": {
      "import/un-resolved": "off",
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal"
            ]
          ]
        }
      ],
      "react/self-closing-comp": [
        "error",
        {
          "component": true,
          "html": true
        }
      ],
      "react/no-unknown-property": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.5.0",
    "autoprefixer": "^9.8.6",
    "customize-cra": "^1.0.0",
    "less-loader": "^6.2.0",
    "react-app-rewired": "^2.1.6",
    "tailwindcss": "^1.6.2"
  }
}
